{
  "name": "react-ts-webpack",
  "version": "1.0.0",
  "description": "basic setting of webpack + ts + react",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js",
    "build:dev": "webpack --config config/webpack.prod.js --env TARGET_ENV=dev",
    "build:prod": "webpack --config config/webpack.prod.js --env TARGET_ENV=prod",
    "preview": "webpack --config config/webpack.prod.js --watch",
    "start": "serve dist/ -s -p 3000",
    "serve": "concurrently \"pnpm preview\" \"pnpm start\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
